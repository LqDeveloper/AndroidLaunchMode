<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.activitydemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.SingleInstanceActivity" android:launchMode="singleInstance"></activity>
        <activity
            android:name=".Activities.SingleTaskActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".Activities.SingleTopActivity"
            android:launchMode="singleTop" />
        <activity android:name=".Activities.NormalReturnActivity" />
        <activity
            android:name=".Activities.StandardActivity"
            android:launchMode="standard" />
        <activity
            android:name=".LaunchActivity"
            android:theme="@style/LaunchTeme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>